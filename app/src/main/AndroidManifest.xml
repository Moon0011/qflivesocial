<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.qingfeng.livesocial">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-sdk tools:overrideLibrary="com.amar.library" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="false"
        android:resizeable="true"
        android:smallScreens="true" />

    <application
        android:name=".common.QFApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.HomeActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.PerfectInfoActivity" />
        <activity android:name=".ui.RecommendActivity" />
        <activity android:name=".ui.RanklistActivity" />
        <activity android:name=".ui.AnchorDetailActivity" />
        <activity android:name=".ui.CallEvaluationAcitivity" />
        <activity android:name=".ui.EvaluationDetailAcitivity" />
        <activity android:name=".ui.MyWalletActivity" />
        <activity android:name=".ui.GiftActivity" />
        <activity android:name=".ui.AnchorAnthenActivity" />
        <activity android:name=".ui.AttentionActivity" />
        <activity android:name=".ui.CallSettingActivity" />
        <activity android:name=".ui.AnchorInfoActivity" />
        <activity android:name=".ui.NicknameActivity" />
        <activity android:name=".ui.LabelsActivity" />
        <activity android:name=".ui.QianmingActivity" />
        <activity android:name=".ui.AnswerIntervalActivity" />
        <activity android:name=".ui.AnchorVideoAnthenActivity" />
        <activity android:name=".ui.BigPicActivity" />
        <activity android:name=".ui.TestActivity" />
    </application>

</manifest>